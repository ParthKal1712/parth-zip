{
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "semi": true,
  "trailingComma": "all",
  "singleQuote": false,
  "tabWidth": 2,
  "plugins": ["prettier-plugin-tailwindcss"],
  "importOrder": [
    "@/components/(.*)$",
    "@/lib/(.*)$",
    "@/routes/(.*)$",
    "@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
